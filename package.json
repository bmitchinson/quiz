{
	"name": "quiz",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env.local -- npx vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"prisma:generate": "npx prisma generate",
		"prisma:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev --name",
		"prisma:migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
		"prisma:studio:dev": "dotenv -e .env.local -- npx prisma studio",
		"prisma:studio:prod": "dotenv -e .env.prod -- npx prisma studio"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"autoprefixer": "^10.4.20",
		"dotenv-cli": "^7.4.2",
		"postcss": "^8.4.45",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.19.1",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.11",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.19.1",
		"@sveltejs/adapter-vercel": "^5.4.3",
		"dotenv": "^16.4.5",
		"jwt-decode": "^4.0.0"
	}
}
